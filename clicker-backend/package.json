{
  "name": "clicker-backend",
  "version": "1.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "NODE_ENV=local nodemon src/server.ts",
    "debug": "nodemon --inspect=9229 src/server.ts",
    "start": "node dist/src/server.js",
    "build": "rimraf dist && tsc -p ./tsconfig.json",
    "migration:up": "npx ts-migrate-mongoose up",
    "migration:down": "npx ts-migrate-mongoose down",
    "migration:create": "npx ts-migrate-mongoose create",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write src",
    "prettier": "prettier . --write --ignore-path .eslintignore",
    "test": "NODE_ENV=local vitest",
    "test:run": "NODE_ENV=local vitest run",
    "seed:top5": "npx ts-node scripts/seedTop5.ts",
    "seed:top50": "npx ts-node scripts/seedTop50.ts",
    "seed:reset": "npx ts-node scripts/seedReset.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Crypto Bot Clicker Backend",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "config": "^4.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "inversify": "^7.10.6",
    "mongoose": "^9.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/node": "^25.0.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-migrate-mongoose": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
